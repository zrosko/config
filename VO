CREATE TABLE [dbo].[NEXGEN_JOB_STATS](
	[STEP_EXECUTION_ID] [bigint] NOT NULL,
	[STEP_NAME] [varchar](100) NULL,
	[JOB_EXECUTION_ID] [bigint] NULL,
	[JOB_INSTANCE_ID] [bigint] NULL,
	[JOB_NAME] [varchar](100) NULL,
	[JOB_DATE] [datetime] NOT NULL,
	[JOB_STATUS] [varchar](10) NULL,
	[TABLE_NAME] [varchar](100) NULL,
	[RAW_COUNT] [bigint] NULL,
	[RAW_EXCLUDED_COUNT] [bigint] NULL,
	[STAGING_COUNT] [bigint] NULL,
	[CURRENT_COUNT] [bigint] NULL,
	[NOT_CURRENT_COUNT] [bigint] NULL,
	[INSERT_COUNT] [bigint] NULL,
	[UPDATE_COUNT] [bigint] NULL,
	[DELETE_COUNT] [bigint] NULL,
	[CREATE_TIMESTAMP] [datetime] NOT NULL
) ON [PRIMARY]
GO

ALTER TABLE [dbo].[NEXGEN_JOB_STATS] ADD  CONSTRAINT [DF_NEXGEN_JOB_STATS_create_timestamp]  DEFAULT (getdate()) FOR [CREATE_TIMESTAMP]
GO
